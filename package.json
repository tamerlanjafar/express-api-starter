{
  "name": "express-api-starter",
  "version": "1.0.0",
  "description": "Rest API starter template for Node.js with Express and PostgreSQL",
  "main": "./src/server.js",
  "scripts": {
    "start": "npm run build-server && npm run migrate && node ./build/server.js",
    "dev": "npm run migrate && nodemon --exec babel-node src/server.js",
    "build-server": "babel -d ./build ./src -s",
    "migrate": "knex migrate:latest"
  },
  "author": "Tamerlan Jafar",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "bcrypt": "^4.0.1",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.1",
    "helmet": "^3.22.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.1",
    "moment": "^2.24.0",
    "morgan": "^1.10.0",
    "pg": "^8.0.2",
    "validator": "^13.0.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-node": "^4.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.4"
  }
}
